  "version": "1.0.0",
  "main": "bundle.js",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "test": "jest",
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon ./server/index.js",
    "db-init": "mysql -u root < ./database/schema.sql",
    "seed-files": "node ./database/seedFiles/dataGenerator.js",
    "seed-db": "mysqlimport -u root --local --lines-terminated-by='*' zelp ./database/seedFiles/users.csv ./database/seedFiles/businesses.csv ./database/seedFiles/reviews.csv",
    "db-start": "npm run db-init && npm run seed-files && npm run seed-db"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "mysql": "^2.16.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "style-loader": "^0.23.0"
  },
  "devDependencies": {
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "jest": "^22.3.0",
    "morgan": "^1.9.0",
    "webpack": "^2.2.1"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
